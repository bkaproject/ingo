<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cavalerie.vlc123.gogogo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="27" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.cavalerie.vlc123.gogogo.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cavalerie.vlc123.gogogo.LoginActivity" />
        <activity android:name="com.cavalerie.vlc123.gogogo.SignActivity" />
        <activity
            android:name="com.cavalerie.vlc123.gogogo.DashboardActivity"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/AppTheme" />
        <activity android:name="com.cavalerie.vlc123.gogogo.UserProfilActivity" />

        <provider
            android:name="com.squareup.picasso.PicassoProvider"
            android:authorities="com.cavalerie.vlc123.gogogo.com.squareup.picasso"
            android:exported="false" />
        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.cavalerie.vlc123.gogogo.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>